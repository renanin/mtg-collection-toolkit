div
  h1 {{set.getName()}}
  md-progress(:md-progress="progress")
  p {{set.uniqueCount()}} / {{set.getCardCount()}} Collected - {{set.totalCount()}} Total - ${{set.totalPrice()}}
  md-button.md-raised.md-primary(@click="save()") Save Changes
  md-button(@click="requestCards()") Request Cards
  md-table
    md-table-row
      md-table-head Name
      md-table-head Price (USD)
    md-table-row(v-for="(card, index) in getCards()",:key="index")
      md-table-cell {{card.name}}
      md-table-cell
        md-button.md-icon-button(@click="increment(index)")
          md-icon add
        p {{card.quantity}}
        md-button.md-icon-button(@click="decrement(index)")
          md-icon remove
      md-table-cell.price-cell ${{card.price}}
