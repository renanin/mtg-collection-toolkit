div
  h1 Cache Management
  h2.md-subhead Total Size: {{convertToMb(cacheSize.total)}}MB
  md-table
    md-table-header
      md-table-row
        md-table-head Category
        md-table-head Size
    md-table-body
      md-table-row(v-for="(size, key) of cacheSize",:key="key")
        md-table-cell {{key}}
        md-table-cell {{convertToMb(size)}}MB
        md-table-cell
          md-button.md-raised.md-primary.md-icon-button(@click="clear(key)")
            md-icon delete
